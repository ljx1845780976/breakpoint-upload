<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.FileHistoryMapper">
    <select id="getFileHistory" resultType="com.entity.FileHistory">
          select * from file_history where username=#{username} and file_name=#{filename}
    </select>
</mapper>
